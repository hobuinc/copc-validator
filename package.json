{
  "name": "@landrush/copc-validator",
  "version": "0.0.1",
  "description": "Reads a file to determine if it's genuine COPC, and provides details about any errors/warnings against the COPC and LAS specs",
  "main": "lib/exports.js",
  "author": "Cameron Woodbury",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "yarn clean && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node lib/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.0.2",
    "@types/lodash": "^4.14.185",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.7.18",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "copc": "^0.0.4",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "piscina": "^3.2.0"
  }
}
