{
  "name": "copc-validator",
  "version": "0.2.1",
  "description": "Reads a file to determine if it's genuine COPC, and provides details about any errors/warnings against the COPC and LAS specs",
  "main": "lib/exports.js",
  "bin": {
    "copcc": "lib/index.js"
  },
  "keywords": [
    "copc",
    "validate",
    "cli"
  ],
  "homepage": "https://github.com/hobuinc/copc-validator#readme.md",
  "repository": "github:hobuinc/copc-validator",
  "author": {
    "name": "Cameron Woodbury",
    "email": "cam@hobu.co"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "yarn clean && tsc --build src && tsc-alias -p src/tsconfig.json && find lib -name '*.test.*' -delete",
    "start": "node lib/index.js",
    "dev": "ts-node --esm -r tsconfig-paths/register src/index.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "lint": "eslint . --ext .ts",
    "flamegraph": "0x --output-dir='.0x/artifacts/{pid}' --output-html='.0x/graphs/{pid}.html' --open lib/index.js",
    "worker": "rollup -c rollup.config.worker.js",
    "prepare": "yarn build && yarn worker"
  },
  "devDependencies": {
    "0x": "^5.4.1",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/cli-progress": "^3.11.0",
    "@types/jest": "^29.0.2",
    "@types/lodash.difference": "^4.5.7",
    "@types/lodash.flatmapdeep": "^4.10.4",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.map": "^4.6.13",
    "@types/lodash.omit": "^4.5.7",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "rollup": "^3.6.0",
    "rollup-plugin-polyfill-node": "^0.11.0",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@landrush/proj4": "^2.7.7",
    "cli-progress": "^3.11.2",
    "copc": "^0.0.4",
    "lodash.difference": "^4.5.0",
    "lodash.flatmapdeep": "^4.10.0",
    "lodash.isequal": "^4.5.0",
    "lodash.map": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "minimist": "^1.2.6",
    "threads": "^1.7.0"
  }
}
